{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/js/*", "/assets/*"]
  },
  "routes": [
    {
      "route": "/.auth/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/auth/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/login/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/redirect/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/callback/*",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
